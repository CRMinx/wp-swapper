{
    "name": "damofd/wp-swapper",
    "description": "Content Swapping Plugin for WordPress",
    "keywords": [
        "wordpress",
        "spa",
        "speed",
        "plugin"
    ],
    "homepage": "https://wpswapper.com",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "WP Swapper",
            "email": "support@wpswapper.com",
            "homepage": "https://wpswapper.com"
        }
    ],
    "type": "wordpress-plugin",
    "support": {
        "issues": "https://github.com/DamoFD/wp-swapper/issues",
        "source": "https://github.com/DamoFD/wp-swapper"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=7.3",
        "composer/installers": "^1.0 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8 || ^9",
        "codedungeon/phpunit-result-printer": "^0.32.0"
    },
    "scripts": {
        "test": "phpunit --bootstrap tests/bootstrap.php tests"
    },
    "autoload": {
        "classmap": [
            "inc/classes"
        ],
        "psr-4": {
            "WP_Swapper\\": "inc/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WP_Swapper\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
